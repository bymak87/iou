<div class="fixed">
  <nav class="top-bar" data-topbar>
    <ul class="title-area">
      <li class="name">
      <h1><a href="#">IOU</a></h1>
    </li>
  </ul>
  <section class="top-bar-section">
    <!-- Right Nav Section -->

  </nav>
</div>
<div class="small-12 small-centered columns">
<title>Welcome</title>
  <h1 class="title">Welcome, <%=current_user.first_name%>!</h1>
  <h5 class="out right">I am not <%= current_user.first_name%>. I want to <%=link_to("Sign Out", "/users/sign_out", :method => "delete")%></h5>

  <%=link_to("Create New Event", "/users/#{current_user.id}/events/new", class: "button success radius")%>
  <%= link_to("Show Event Details", "/users/#{current_user.id}/events", class: "button radius")%>

  <div class="notebook small-6 small-centered columns">

    <div class="page">
      <h2 class="head">Events</h2>
      <br />
    <% current_user.events.each do |event| %>
        <li><%=event.name.capitalize%>: <%=event.friend%> </li>
        <br />
    <% end %>
    </div>
  </div>

</div>






